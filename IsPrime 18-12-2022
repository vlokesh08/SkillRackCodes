#include <bits/stdc++.h>
 
using namespace std;
bool isprime(int n){
    if(n<=1) return false;
    if(n==2) return true;
    for(int i=2;i<n;i++){
        if(n%i==0) return false;
    }
    return true;
}
int main(int argc, char** argv)
{
    int n;
    cin>>n;
    if(isprime(n) || n%2==0) cout<<"valid";
    else cout<<"invalid";

}
